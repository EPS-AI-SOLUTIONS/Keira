{
  "name": "keira-watermark-remover",
  "private": true,
  "version": "2.0.0",
  "description": "AI-powered video watermark removal - Regis Architecture",
  "author": "Pawel Serkowski",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "kill-port": "node scripts/kill-port.cjs 5175",
    "predev": "pnpm kill-port",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "backend": "python -m uvicorn api.main:app --host 127.0.0.1 --port 8002 --reload",
    "kill-port:backend": "node scripts/kill-port.cjs 8002",
    "prebackend": "pnpm kill-port:backend",
    "start": "concurrently \"pnpm backend\" \"pnpm dev\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "audit:frontend": "npm audit",
    "audit:backend": "pip-audit -r api/requirements.txt",
    "audit": "concurrently \"npm run audit:frontend\" \"npm run audit:backend\"",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "format": "biome format --write ."
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "biome check --write"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "clsx": "^2.1.1",
    "framer-motion": "^12.31.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.1",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-links": "^10.2.0",
    "@storybook/react": "^10.2.0",
    "@storybook/react-vite": "^10.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.2.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "storybook": "^10.2.0",
    "tailwindcss": "^4.1.18",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.18"
  }
}
